{
  "name": "xaegir-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt dev --trace-warnings",
    "build": "nuxt build",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "lint:js": "eslint --ext \".js,.ts,.vue\" --ignore-path .gitignore .",
    "lint:style": "stylelint \"**/*.{css,scss,sass,html,vue}\" --ignore-path .gitignore",
    "lint:prettier": "prettier --check .",
    "lint": "yarn lint:js && yarn lint:style && yarn lint:prettier",
    "lintfix": "prettier --write --list-different . && yarn lint:js --fix && yarn lint:style --fix",
    "prepare": "cd ../../.. && husky install cmd/web/app/.husky",
    "test": "jest",
    "contracts-lint": "solhint **/*.sol",
    "contracts-compile": "node ./bin/main.js compile",
    "contracts-test": "node ./bin/main.js tests",
    "contracts-perf": "node ./bin/main.js perf",
    "vca": "vca --dir pages",
    "vitebook:build": "vitebook build",
    "vitebook:dev": "vitebook dev",
    "vitebook:preview": "vitebook preview"
  },
  "lint-staged": {
    "*.{js,ts,vue}": "eslint --cache",
    "*.{css,scss,sass,html,vue}": "stylelint",
    "*.**": "prettier --check --ignore-unknown"
  },
  "dependencies": {
    "@nuxtjs/composition-api": "^0.32.0",
    "@pinia/nuxt": "^0.4.0",
    "pinia": "^2.0.14",
    "web3modal": "^1.9.3"
  },
  "//": {},
  "devDependencies": {
    "@babel/eslint-parser": "^7.18.2",
    "@commitlint/cli": "^17.0.2",
    "@commitlint/config-conventional": "^17.0.2",
    "@intlify/nuxt3": "^0.2.2",
    "@nuxt/content": "^2.0.0",
    "@nuxtjs/color-mode": "^3.1.4",
    "@nuxtjs/eslint-config-typescript": "^10.0.0",
    "@nuxtjs/strapi": "^1.4.0",
    "@nuxtjs/tailwindcss": "^5.1.2",
    "@tkskto/vue-component-analyzer": "^0.5.1",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "@vitebook/client": "^0.100.5",
    "@vitebook/core": "^0.100.5",
    "@vitebook/markdown-shiki": "^0.100.5",
    "@vitebook/markdown-vue": "^0.100.5",
    "@vitebook/theme-default": "^0.100.5",
    "@vitebook/vue": "^0.100.5",
    "@vue/compiler-sfc": "^3.2.37",
    "@vue/eslint-config-standard": "^7.0.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^28.1.1",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-nuxt": "^3.2.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^9.1.1",
    "husky": "^7.0.4",
    "jest": "^27.4.4",
    "lint-staged": "^12.1.2",
    "nuxt": "^3.0.0-rc.4",
    "nuxt-lodash": "^2.2.0",
    "postcss": "^8.4.4",
    "postcss-html": "^1.3.0",
    "prettier": "^2.7.0",
    "prettier-plugin-solidity": "^1.0.0-beta.19",
    "stylelint": "^14.1.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended-vue": "^1.1.0",
    "stylelint-config-standard": "^24.0.0",
    "svelte": "^3.49.0",
    "ts-jest": "^28.0.5",
    "typescript": "^4.7.3",
    "vite": "^3.0.9",
    "vite-plugin-eslint": "^1.6.1",
    "vue-jest": "^3.0.7"
  }
}
